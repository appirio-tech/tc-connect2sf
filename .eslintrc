{
  "parser": "babel-eslint",
  "extends": [
    "airbnb-base"
  ],
  "plugins": [
    "babel"
  ],
  "env": {
    "node": true
  },
  "globals": {
  },
  "rules": {
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "object-curly-spacing": 0,
    "generator-star-spacing": 0,
    "max-len": [2, 120],
    "import/prefer-default-export": 0
  }
}
